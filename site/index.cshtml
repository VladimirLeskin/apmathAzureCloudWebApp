@{
    var txt = DateTime.Now; // ëþáîé êîä íà
    ApmathAzureCloudLarisa.Controllers.HomeController obj = new ApmathAzureCloudLarisa.Controllers.HomeController();
    //double[] d = obj.Haha();
    double res = 0;
    double[] x;


    if (IsPost)
    {
        string line = Request.Form["Values"];
        string[] values=line.Split(new Char[] { ',', ' '});
        x = new double[values.Length];
        for (int i = 0; i < values.Length; i++)
        {
            x[i] = Convert.ToDouble(values[i]);
            res += x[i];
        }
        res /= x.Length;
    }
}

